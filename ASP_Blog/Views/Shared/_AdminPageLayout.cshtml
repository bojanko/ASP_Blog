<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
</head>
<body>
    <!-- NAVBAR AND FOOTER -->
    @{
        Html.RenderPartial("~/Views/Shared/_NavbarAdmin.cshtml");

        Html.RenderPartial("~/Views/Shared/_Footer.cshtml");
    }

    <br />

    <!-- MAIN CONTENT -->
    <div class="container">
      <div class="row">
        <div class="col-md-8">
          @RenderSection("content", required: false)
        </div>
        <div class="col-md-4">
            @RenderSection("sidebar", required: false)
        </div>
      </div>
    </div>

    @RenderBody() 

    <br />
    <br />
    <br />

    @Scripts.Render("~/bundles/bootstrap")
</body>
</html>
